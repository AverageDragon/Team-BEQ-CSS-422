000001EA                           147  ; "printSize" Helper
000001EA                           148  ; Gary
000001EA                           149  ;
000001EA                           150  ; 1st argument is the data register with opcode whose size is being decoded
000001EA                           151  ;
000001EA                           152  ; Used in opcodes whose size bits are #13 and #12 (e.g. MOVE). It does so
000001EA                           153  ; by sequentally checking both bits. Bit #13 differentiates byte and word size.
000001EA                           154  ; If bit goes to word, bit #12 is checked to differntiate word and long word.
000001EA                           155  ; Once matched, size is printed and appended to already printed opcode (TRAP #15 no CR,LF).
000001EA                           156  ; ------------------------------------------------------------
000001EA                           157  printSize:  MACRO
000001EA                           158          MOVE.W  \1, D3      ; input value      
000001EA                           159  checkForB\@: 
000001EA                           160          BTST    #13, D3      ; Check if contains 0
000001EA                           161          BNE     checkForW\@  ; If not go to word size
000001EA                           162          LEA     oBSize, A1   ; If bit #13 is 0, size is byte and macro ends. 
000001EA                           163          MOVE.B  #14,D0          
000001EA                           164          TRAP    #15
000001EA                           165          BRA     complete\@      
000001EA                           166  checkForW\@: 
000001EA                           167          BTST    #12, D3      ; Check if contains 1
000001EA                           168          BEQ     checkForL\@  ; If not, size cannot be word
000001EA                           169          LEA     oWSize, A1   ; If bit #12 is 1, size is word and macro ends.  
000001EA                           170          MOVE.B  #14,D0          
000001EA                           171          TRAP    #15
000001EA                           172          BRA     complete\@  
000001EA                           173  checkForL\@:
000001EA                           174          LEA     oLSize, A1   ; If bit #12 is 0, size is longword and macro ends.  
000001EA                           175          MOVE.B  #14,D0          
000001EA                           176          TRAP    #15
000001EA                           177  complete\@:  ; Size has been printed
000001EA                           178          ENDM
