00001286  3612                     783m         MOVE.W  (A2), D3      ; INPUT VALUE
00001288  123C 000F                784m         MOVE.B  #15, D1     ; COUNTER TRACKING THE BIT POSITION OF D3
0000128C                           785m         
0000128C                           786m LOOP_21: 
0000128C  0C11 0030                787m         CMP.B   #$30, (A1)  ; CHECK IF THE TEMPLATE WANTS A 0
00001290  6700 000E                788m         BEQ     CHECKFOR0_21
00001294  0C11 0031                789m         CMP.B   #$31, (A1)  ; CHECK IF THE TEMPLATE WANTS A 1
00001298  6700 0010                790m         BEQ     CHECKFOR1_21
0000129C  6000 0012                791m         BRA     NEXT_21      ; TEMPLATE HAS A WILDCARD, GO TO NEXT ITERATION
000012A0                           792m         
000012A0                           793m CHECKFOR0_21: 
000012A0  0303                     794m         BTST    D1, D3      ; CHECK IF CONTAINS 0
000012A2  6600 291A                795m         BNE     OUTPUTDATA  ; MISSING A 0 -> NOT A VALID OPCODE
000012A6  6000 0008                796m         BRA     NEXT_21
000012AA                           797m 
000012AA                           798m CHECKFOR1_21:
000012AA  0303                     799m         BTST    D1, D3      ; CHECK IF CONTAINS 1
