000001EA                           181  ; "printSizeLow" Helper
000001EA                           182  ; Gary
000001EA                           183  ;
000001EA                           184  ; 1st argument is the data register with opcode whose size is being decoded
000001EA                           185  ;
000001EA                           186  ; Used in opcodes whose size bits are #6 and #7 (e.g. ADD). It does so
000001EA                           187  ; by sequentally checking both bits. Bit #6 differentiates byte and word size.
000001EA                           188  ; If bit goes to word, bit #7 is checked to differntiate word and long word.
000001EA                           189  ; Once matched, size is printed and appended to already printed opcode, macro exits.
000001EA                           190  ; ------------------------------------------------------------
000001EA                           191  printSizeLow:  MACRO
000001EA                           192          MOVE.W  \1, D3      ; input value      
000001EA                           193  checkForB\@: 
000001EA                           194          BTST    #6,  D3      ; Check if contains 0
000001EA                           195          BNE     checkForW\@  ; If not go to word size
000001EA                           196          LEA     oBSize, A1     
000001EA                           197          MOVE.B  #14,D0          
000001EA                           198          TRAP    #15
000001EA                           199          BRA     complete\@      
000001EA                           200  checkForW\@: 
000001EA                           201          BTST    #7, D3      ; Check if contains 0
000001EA                           202          BNE     checkForL\@ ; If not go to longword size
000001EA                           203          LEA     oWSize, A1     
000001EA                           204          MOVE.B  #14,D0          
000001EA                           205          TRAP    #15
000001EA                           206          BRA     complete\@  
000001EA                           207  checkForL\@:
000001EA                           208          LEA     oLSize, A1   ; If both bits are #1, size is longword and macro ends.  
000001EA                           209          MOVE.B  #14,D0          
000001EA                           210          TRAP    #15
000001EA                           211  complete\@:  ; Size has been printed
000001EA                           212          ENDM
