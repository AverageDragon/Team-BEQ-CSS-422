000012B0                           801m 
000012B0                           802m NEXT_21: 
000012B0  B23C 0000                803m         CMP.B   #0, D1      ; COMPARE BIT COUNTER TO 0
000012B4  6700 0008                804m         BEQ     COMPLETE_21  ; BRANCH TO COMPLETE IF BIT COUNTER REACHED 0
000012B8  5301                     805m         SUB.B   #1, D1      ; CONTINUING LOOP -> DECREMENT BIT COUNTER
000012BA  1819                     806m         MOVE.B  (A1)+, D4   ; MOVE A1 TO THE NEXT POINTER (D3 IRRELEVANT)
000012BC  60CE                     807m         BRA     LOOP_21      ; BEGIN LOOP AGAIN
000012BE                           808m 
