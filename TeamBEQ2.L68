00001198                           658  ; ========================================================================
00001198                           659  ; "identifyOpcode" subroutine
00001198                           660  ; Jonathan
00001198                           661  ; 
00001198                           662  ; Reads the Word at A2 and determines which opcode it represents.
00001198                           663  ; When the opcode is identified, the appropriate "decodeX" subroutine
00001198                           664  ; is invoked.
00001198                           665  ;
00001198                           666  ; Uses a generated Trie structure to determine where to split on which bits
00001198                           667  ; in order to efficiently identify the exact opcode
00001198                           668  ; ========================================================================
00001198                           669  identifyOpcode:
00001198  3612                     670          MOVE.W  (A2), D3    ; Move the value at A2 into D3 for bit testing
0000119A  0803 000F                671          BTST    #$F, D3     ; Check the 1st bit (from the left)
0000119E  6700 0006                672          BEQ     split0      ; Split based on the first bit
000011A2  6000 006E                673          BRA     split1
000011A6                           674  
000011A6                           675  split0: 
000011A6  0803 000E                676          BTST    #$E, D3     ; Check the 2nd bit
000011AA  6700 011E                677          BEQ     decodeMOVE  ; MOVE follows 00+
000011AE  6000 0002                678          BRA     split01
000011B2                           679          
000011B2                           680  split01:
000011B2  0803 000D                681          BTST    #$D, D3     ; Check the 3rd bit
000011B6  6700 0006                682          BEQ     split0100   ; Code is 010, but all subcodes are 0100
000011BA  6000 0032                683          BRA     split0110   ; Code is 011, but all subcodes are 0110        
000011BE                           684  
000011BE                           685  split0100:
000011BE  0803 0008                686          BTST    #$8, D3     ; Check the 8th bit for LEA
000011C2  6600 1756                687          BNE     decodeLEA   ; LEA follows 0100XXX1+
000011C6  0803 000B                688          BTST    #$B, D3     ; Check the 5th bit for NOT
000011CA  6700 1FB4                689          BEQ     decodeNOT   ; NOT follows 01000+
000011CE  0803 0009                690          BTST    #$9, D3     ; Check the 7th bit
000011D2  6700 05E0                691          BEQ     decodeMOVEM ; MOVEM follows 01001x0+
000011D6  6000 0002                692          BRA     split0100111
000011DA                           693  
000011DA                           694  split0100111:
000011DA  0803 0007                695          BTST    #$7, D3     ; Check the 9th bit for JSR
000011DE  6600 26D6                696          BNE     decodeJSR   ; JSR follows 010011101+
000011E2  0803 0002                697          BTST    #$2, D3     ; Check the 14th bit
000011E6  6700 009A                698          BEQ     decodeNOP   ; NOP follows 01001110011100+
000011EA  6000 294C                699          BRA     decodeRTS   ; RTS follows 01001110011101+
000011EE                           700  
000011EE                           701  split0110:
000011EE  0803 000B                702          BTST    #$B, D3     ; Check the 5th bit
000011F2  6700 0006                703          BEQ     split01100
000011F6  6000 000E                704          BRA     split01101
000011FA                           705  
000011FA                           706  split01100:
000011FA  0803 000A                707          BTST    #$A, D3     ; Check the 6th bit
000011FE  6700 2980                708          BEQ     decodeBRA   ; BRA starts with 011000
00001202  6000 25F8                709          BRA     decodeBEQ   ; BEQ starts with 011001
00001206                           710          
00001206                           711  split01101:
00001206  0803 0008                712          BTST    #$8, D3     ; Check the 8th bit
0000120A  6700 262E                713          BEQ     decodeBGE   ; BGE starts with 01101100
0000120E  6000 2668                714          BRA     decodeBLT   ; BLT starts with 01101101
00001212                           715          
00001212                           716  split1: 
00001212  0803 000E                717          BTST    #$E, D3     ; Check the 2nd bit
00001216  6700 0006                718          BEQ     split10
0000121A  6000 000E                719          BRA     split11
0000121E                           720  
0000121E                           721  split10:
0000121E  0803 000C                722          BTST    #$C, D3     ; Check the 4th bit
00001222  6700 1424                723          BEQ     decodeDIVU  ; DIVU follows 1000+
00001226  6000 0B8C                724          BRA     decodeSUB   ; SUB follows 1001+
0000122A                           725  
0000122A                           726  split11:
0000122A  0803 000D                727          BTST    #$D, D3     ; Check the 3rd bit
0000122E  6700 0006                728          BEQ     split110
00001232  6000 0026                729          BRA     split111
00001236                           730  
00001236                           731  split110:
00001236  0803 000C                732          BTST    #$C, D3     ; Check the 4th bit for ADD
0000123A  6600 05B6                733          BNE     decodeADD   ; ADD follows 1101+
0000123E  0803 0008                734          BTST    #$8, D3     ; MULS.W has all 3 bits at 8, 9, and 10 set
00001242  6700 197A                735          BEQ     decodeAND   ; If any of these BTSTs show a 0, opcode is AND
00001246  0803 0007                736          BTST    #$7, D3
0000124A  6700 1972                737          BEQ     decodeAND
0000124E  0803 0006                738          BTST    #$6, D3
00001252  6700 196A                739          BEQ     decodeAND
00001256  6000 111E                740          BRA     decodeMULS  ; All 3 positions (8, 9, 10) were set
0000125A                           741  
0000125A                           742  split111: 
0000125A  0803 0003                743          BTST    #$3, D3     ; Check 13th bit
0000125E  6700 0012                744          BEQ     ariShift ; ASL_ASR Memory Shift follows 1110000x11+
00001262                           745     
00001262  0803 0005                746          BTST    #$5, D3     
00001266  6600 0006                747          BNE     logReg      
0000126A  6000 21C4                748          BRA     decodeLSx_M
0000126E                           749        
0000126E  6000 2294                750  logReg: BRA     decodeLSx_R
00001272                           751  
00001272                           752  ariShift: ; Special split specifically for Arithmetic Shift
00001272  0803 0005                753          BTST    #$5, D3     
00001276  6600 0006                754          BNE     ariReg
0000127A  6000 239A                755          BRA     decodeASx_M    
0000127E  6000 246A                756  ariReg: BRA     decodeASx_R
