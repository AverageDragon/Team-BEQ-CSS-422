000001EA                           106  ; "verifyOpcode" Helper
000001EA                           107  ; Jonathan
000001EA                           108  ;
000001EA                           109  ; 1st argument is a template string (e.g. "0100...111......" for LEA)
000001EA                           110  ; 2nd argument is a data register to check against the template
000001EA                           111  ;
000001EA                           112  ; Verifies that the opcode passed through \2 matches the pattern of \1
000001EA                           113  ; If they do not match, branches to outputData
000001EA                           114  ; ------------------------------------------------------------
000001EA                           115  verifyOpcode:  MACRO
000001EA                           116          LEA     \1, A1      ; Input pattern string
000001EA                           117          MOVE.W  \2, D3      ; input value
000001EA                           118          MOVE.B  #15, D1     ; Counter tracking the bit position of D3
000001EA                           119          
000001EA                           120  loop\@: 
000001EA                           121          CMP.B   #$30, (A1)  ; Check if the template wants a 0
000001EA                           122          BEQ     checkFor0\@
000001EA                           123          CMP.B   #$31, (A1)  ; Check if the template wants a 1
000001EA                           124          BEQ     checkFor1\@
000001EA                           125          BRA     next\@      ; Template has a wildcard, go to next iteration
000001EA                           126          
000001EA                           127  checkFor0\@: 
000001EA                           128          BTST    D1, D3      ; Check if contains 0
000001EA                           129          BNE     outputData  ; Missing a 0 -> Not a valid opcode
000001EA                           130          BRA     next\@
000001EA                           131  
000001EA                           132  checkFor1\@:
000001EA                           133          BTST    D1, D3      ; Check if contains 1
000001EA                           134          BEQ     outputData  ; Missing a 1 -> Not a valid opcode
000001EA                           135  
000001EA                           136  next\@: 
000001EA                           137          CMP.B   #0, D1      ; Compare bit counter to 0
000001EA                           138          BEQ     complete\@  ; Branch to complete if bit counter reached 0
000001EA                           139          SUB.B   #1, D1      ; Continuing loop -> Decrement bit counter
000001EA                           140          MOVE.B  (A1)+, D4   ; Move A1 to the next pointer (D3 irrelevant)
000001EA                           141          BRA     loop\@      ; Begin loop again
000001EA                           142  
000001EA                           143  complete\@:  ; Opcode has been verified
000001EA                           144          ENDM
