0000024A                           182  ; "verifyOpcode" Helper
0000024A                           183  ; Jonathan
0000024A                           184  ;
0000024A                           185  ; 1st argument is a template string (e.g. "0100...111......" for LEA)
0000024A                           186  ; 2nd argument is a data register to check against the template
0000024A                           187  ;
0000024A                           188  ; Verifies that the opcode passed through \2 matches the pattern of \1
0000024A                           189  ; If they do not match, branches to outputData
0000024A                           190  ; ------------------------------------------------------------
0000024A                           191  verifyOpcode:  MACRO
0000024A                           192          LEA     \1, A1      ; Input pattern string
0000024A                           193          MOVE.W  \2, D3      ; input value
0000024A                           194          MOVE.B  #15, D1     ; Counter tracking the bit position of D3
0000024A                           195          
0000024A                           196  loop\@: 
0000024A                           197          CMP.B   #$30, (A1)  ; Check if the template wants a 0
0000024A                           198          BEQ     checkFor0\@
0000024A                           199          CMP.B   #$31, (A1)  ; Check if the template wants a 1
0000024A                           200          BEQ     checkFor1\@
0000024A                           201          BRA     next\@      ; Template has a wildcard, go to next iteration
0000024A                           202          
0000024A                           203  checkFor0\@: 
0000024A                           204          BTST    D1, D3      ; Check if contains 0
0000024A                           205          BNE     outputData  ; Missing a 0 -> Not a valid opcode
0000024A                           206          BRA     next\@
0000024A                           207  
0000024A                           208  checkFor1\@:
0000024A                           209          BTST    D1, D3      ; Check if contains 1
0000024A                           210          BEQ     outputData  ; Missing a 1 -> Not a valid opcode
0000024A                           211  
0000024A                           212  next\@: 
0000024A                           213          CMP.B   #0, D1      ; Compare bit counter to 0
0000024A                           214          BEQ     complete\@  ; Branch to complete if bit counter reached 0
0000024A                           215          SUB.B   #1, D1      ; Continuing loop -> Decrement bit counter
0000024A                           216          MOVE.B  (A1)+, D4   ; Move A1 to the next pointer (D3 irrelevant)
0000024A                           217          BRA     loop\@      ; Begin loop again
0000024A                           218  
0000024A                           219  complete\@:  ; Opcode has been verified
0000024A                           220          ENDM
