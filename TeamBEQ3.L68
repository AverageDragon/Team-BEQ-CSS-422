0000024A                           306  checkForDestDn\@: 
0000024A                           307          BTST    D1, D3      
0000024A                           308          BNE     checkForSrcDn\@
0000024A                           309          JSR     next\@
0000024A                           310          MOVE.B  #$1, D6
0000024A                           311          BTST    D1, D3      
0000024A                           312          BEQ     DestDnW\@
0000024A                           313  DestDnL\@:        
0000024A                           314          LEA     oLSize, A1     
0000024A                           315          MOVE.B  #14,D0          
0000024A                           316          TRAP    #15
0000024A                           317          BRA     complete\@ 
0000024A                           318  DestDnW\@: 
0000024A                           319          JSR     next\@
0000024A                           320          BTST    D1, D3      
0000024A                           321          BEQ     DestDnB\@
0000024A                           322          LEA     oWSize, A1     
0000024A                           323          MOVE.B  #14,D0          
0000024A                           324          TRAP    #15
0000024A                           325          BRA     complete\@ 
0000024A                           326  DestDnB\@: 
0000024A                           327          LEA     oBSize, A1     
0000024A                           328          MOVE.B  #14,D0          
0000024A                           329          TRAP    #15
0000024A                           330          BRA     complete\@ 
0000024A                           331  checkForSrcDn\@: 
0000024A                           332          JSR     next\@
0000024A                           333          BTST    D1, D3      
0000024A                           334          BEQ     SrcDnW\@
0000024A                           335  SrcDnL\@:        
0000024A                           336          LEA     oLSize, A1     
0000024A                           337          MOVE.B  #14,D0          
0000024A                           338          TRAP    #15
0000024A                           339          printDnDest D3
0000024A                           340          BRA     complete\@ 
0000024A                           341  SrcDnW\@:
0000024A                           342          JSR     next\@
0000024A                           343          BTST    D1, D3     
0000024A                           344          BEQ     SrcDnB\@
0000024A                           345          LEA     oWSize, A1     
0000024A                           346          MOVE.B  #14,D0          
0000024A                           347          TRAP    #15
0000024A                           348          printDnDest D3
0000024A                           349          BRA     complete\@ 
