00001162                           729  ; "identifyOpcode" subroutine
00001162                           730  ; Jonathan
00001162                           731  ; 
00001162                           732  ; Reads the Word at A2 and determines which opcode it represents.
00001162                           733  ; When the opcode is identified, the appropriate "decodeX" subroutine
00001162                           734  ; is invoked.
00001162                           735  ;
00001162                           736  ; Uses a generated Trie structure to determine where to split on which bits
00001162                           737  ; in order to efficiently identify the exact opcode
00001162                           738  ; ========================================================================
00001162                           739  identifyOpcode:
00001162  3612                     740          MOVE.W  (A2), D3    ; Move the value at A2 into D3 for bit testing
00001164  0803 000F                741          BTST    #$F, D3     ; Check the 1st bit (from the left)
00001168  6700 0006                742          BEQ     split0      ; Split based on the first bit
0000116C  6000 006E                743          BRA     split1
00001170                           744  
00001170                           745  split0: 
00001170  0803 000E                746          BTST    #$E, D3     ; Check the 2nd bit
00001174  6700 011E                747          BEQ     decodeMOVE  ; MOVE follows 00+
00001178  6000 0002                748          BRA     split01
0000117C                           749          
0000117C                           750  split01:
0000117C  0803 000D                751          BTST    #$D, D3     ; Check the 3rd bit
00001180  6700 0006                752          BEQ     split0100   ; Code is 010, but all subcodes are 0100
00001184  6000 0032                753          BRA     split0110   ; Code is 011, but all subcodes are 0110        
00001188                           754  
00001188                           755  split0100:
00001188  0803 0008                756          BTST    #$8, D3     ; Check the 8th bit for LEA
0000118C  6600 1756                757          BNE     decodeLEA   ; LEA follows 0100XXX1+
00001190  0803 000B                758          BTST    #$B, D3     ; Check the 5th bit for NOT
00001194  6700 1FB4                759          BEQ     decodeNOT   ; NOT follows 01000+
00001198  0803 0009                760          BTST    #$9, D3     ; Check the 7th bit
0000119C  6700 05E0                761          BEQ     decodeMOVEM ; MOVEM follows 01001x0+
000011A0  6000 0002                762          BRA     split0100111
000011A4                           763  
000011A4                           764  split0100111:
000011A4  0803 0007                765          BTST    #$7, D3     ; Check the 9th bit for JSR
000011A8  6600 26D6                766          BNE     decodeJSR   ; JSR follows 010011101+
000011AC  0803 0002                767          BTST    #$2, D3     ; Check the 14th bit
000011B0  6700 009A                768          BEQ     decodeNOP   ; NOP follows 01001110011100+
000011B4  6000 294C                769          BRA     decodeRTS   ; RTS follows 01001110011101+
000011B8                           770  
000011B8                           771  split0110:
000011B8  0803 000B                772          BTST    #$B, D3     ; Check the 5th bit
000011BC  6700 0006                773          BEQ     split01100
000011C0  6000 000E                774          BRA     split01101
000011C4                           775  
000011C4                           776  split01100:
000011C4  0803 000A                777          BTST    #$A, D3     ; Check the 6th bit
000011C8  6700 2980                778          BEQ     decodeBRA   ; BRA starts with 011000
000011CC  6000 25F8                779          BRA     decodeBEQ   ; BEQ starts with 011001
000011D0                           780          
000011D0                           781  split01101:
000011D0  0803 0008                782          BTST    #$8, D3     ; Check the 8th bit
000011D4  6700 262E                783          BEQ     decodeBGE   ; BGE starts with 01101100
000011D8  6000 2668                784          BRA     decodeBLT   ; BLT starts with 01101101
000011DC                           785          
000011DC                           786  split1: 
000011DC  0803 000E                787          BTST    #$E, D3     ; Check the 2nd bit
000011E0  6700 0006                788          BEQ     split10
000011E4  6000 000E                789          BRA     split11
000011E8                           790  
000011E8                           791  split10:
000011E8  0803 000C                792          BTST    #$C, D3     ; Check the 4th bit
000011EC  6700 1424                793          BEQ     decodeDIVU  ; DIVU follows 1000+
000011F0  6000 0B8C                794          BRA     decodeSUB   ; SUB follows 1001+
000011F4                           795  
000011F4                           796  split11:
000011F4  0803 000D                797          BTST    #$D, D3     ; Check the 3rd bit
000011F8  6700 0006                798          BEQ     split110
000011FC  6000 0026                799          BRA     split111
00001200                           800  
00001200                           801  split110:
00001200  0803 000C                802          BTST    #$C, D3     ; Check the 4th bit for ADD
00001204  6600 05B6                803          BNE     decodeADD   ; ADD follows 1101+
00001208  0803 0008                804          BTST    #$8, D3     ; MULS.W has all 3 bits at 8, 9, and 10 set
0000120C  6700 197A                805          BEQ     decodeAND   ; If any of these BTSTs show a 0, opcode is AND
00001210  0803 0007                806          BTST    #$7, D3
00001214  6700 1972                807          BEQ     decodeAND
00001218  0803 0006                808          BTST    #$6, D3
0000121C  6700 196A                809          BEQ     decodeAND
00001220  6000 111E                810          BRA     decodeMULS  ; All 3 positions (8, 9, 10) were set
00001224                           811  
00001224                           812  split111: 
00001224  0803 0003                813          BTST    #$3, D3     ; Check 13th bit
00001228  6700 0012                814          BEQ     ariShift ; ASL_ASR Memory Shift follows 1110000x11+
0000122C                           815     
